{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "no-unused-vars":"warn",
        "linebreak-style":"off",
        "no-underscore-dangle":"off"
    }
}
